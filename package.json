{
  "name": "christoph-halstenberg-frontend",
  "version": "1.0.0",
  "description": "This is a Frontend Theme tusing React with Webpack and Babel.",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "babel-node ./node_modules/webpack-dev-server/bin/webpack-dev-server --inline --progress --config webpack.dev.js",
    "clean": "rimraf ./build/*",
    "cleannode": "rimraf ./node_modules && npm install",
    "deploy": "rimraf ./firebase/public/* && cp -r ./build/* ./firebase/public/ && cd ./firebase/ && firebase deploy",
    "build": "npm run clean && babel-node ./node_modules/webpack/bin/webpack --progress --optimize-minimize --config webpack.prod.js"
  },
  "keywords": [
    "react",
    "webpack",
    "babel",
    "materialdesign"
  ],
  "author": "BS",
  "license": "MIT",
  "devDependencies": {
